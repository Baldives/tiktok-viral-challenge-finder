{
  "actorOutputSchemaVersion": 1,
  "title": "Top TikTok Viral Challenges",
  "description": "Ranked list of currently exploding TikTok hashtag challenges with estimated 7-day views, virality score, and example videos.",
  "type": "object",
  "properties": {
    "results": {
      "title": "Challenges",
      "description": "List of top TikTok viral challenges",
      "type": "array",
      "template": "{{#each this}}{{Rank}}. {{Challenge}} - {{Title}} ({{Est. Views (7d)}} views, Score {{Virality Score}})\n{{Example}}\n\n{{/each}}",
      "items": {
        "type": "object",
        "properties": {
          "Rank": { "title": "Rank", "type": "integer" },
          "Challenge": { "title": "Hashtag", "type": "string" },
          "Title": { "title": "Challenge Title", "type": "string" },
          "Est. Views (7d)": { "title": "Est. Views (7d)", "type": "string" },
          "Virality Score": { "title": "Virality Score", "type": "integer" },
          "Example": { "title": "Example Video URL", "type": "string" }
        },
        "required": ["Rank", "Challenge", "Title", "Est. Views (7d)", "Virality Score", "Example"],
        "template": "{{Rank}}. {{Challenge}} - {{Title}} ({{Est. Views (7d)}} views, Score {{Virality Score}})\n{{Example}}\n"
      }
    }
  },
  "required": ["results"]
}
